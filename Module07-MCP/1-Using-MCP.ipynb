      "source": [
        "# Simulated Filesystem MCP Tools (for demonstration)\n",
        "# In production, these would come from actual MCP server\n",
        "\n",
        "@tool\n",
        "def read_file(file_path: str) -> str:\n",
        "    \"\"\"\n",
        "    Read file content from filesystem.\n",
        "    \n",
        "    Args:\n",
        "        file_path: Path to file (e.g., './data/resumes/priya_sharma.pdf')\n",
        "    \n",
        "    Returns:\n",
        "        File content or error message\n",
        "    \"\"\"\n",
        "    # Simulated - in production this would use actual MCP filesystem server\n",
        "    if \"priya_sharma\" in file_path:\n",
        "        return SAMPLE_RESUME\n",
        "    elif \"property_photos\" in file_path:\n",
        "        return \"[PHOTO_DATA] Goa Beach Villa - 5 photos available\"\n",
        "    elif \"contract\" in file_path:\n",
        "        return \"\"\"\n",
        "RENTAL AGREEMENT - Goa Beach Villa\n",
        "Property: PROP001\n",
        "Guest: Sarah Johnson\n",
        "Duration: Oct 20-25, 2025\n",
        "Total: â‚¹42,500\n",
        "Terms: Standard rental terms apply...\n",
        "\"\"\"\n",
        "    return f\"File not found: {file_path}\"\n",
        "\n",
        "@tool\n",
        "def write_file(file_path: str, content: str) -> str:\n",
        "    \"\"\"\n",
        "    Write content to file.\n",
        "    \n",
        "    Args:\n",
        "        file_path: Destination path\n",
        "        content: Content to write\n",
        "    \n",
        "    Returns:\n",
        "        Confirmation message\n",
        "    \"\"\"\n",
        "    return f\"âœ“ File written: {file_path} ({len(content)} bytes)\"\n",
        "\n",
        "@tool\n",
        "def list_files(directory: str) -> str:\n",
        "    \"\"\"\n",
        "    List files in directory.\n",
        "    \n",
        "    Args:\n",
        "        directory: Directory path\n",
        "    \n",
        "    Returns:\n",
        "        List of files\n",
        "    \"\"\"\n",
        "    # Simulated directory listings\n",
        "    listings = {\n",
        "        \"./data/resumes\": [\"priya_sharma.pdf\", \"arjun_mehta.pdf\", \"candidate_template.docx\"],\n",
        "        \"./data/properties\": [\"prop001_photos/\", \"prop002_photos/\", \"prop003_photos/\"],\n",
        "        \"./data/contracts\": [\"BOOK001_contract.pdf\", \"BOOK002_contract.pdf\"]\n",
        "    }\n",
        "    files = listings.get(directory, [])\n",
        "    return json.dumps({\"directory\": directory, \"files\": files}, indent=2)\n",
        "\n",
        "# Create agent with filesystem tools\n",
        "filesystem_tools = [read_file, write_file, list_files]\n",
        "\n",
        "filesystem_agent = create_react_agent(\n",
        "    llm,\n",
        "    tools=filesystem_tools,\n",
        "    state_modifier=\"\"\"\n",
        "You are a Document Management Assistant.\n",
        "\n",
        "You help with:\n",
        "- Reading and organizing resumes (HR use case)\n",
        "- Managing property documents and photos (AirBnb use case)\n",
        "- Creating reports and contracts\n",
        "\n",
        "Always provide clear file paths and organize files systematically.\n",
        "\"\"\"\n",
        ")\n",
        "\n",
        "print(\"âœ“ Filesystem MCP Agent created with tools:\")\n",
        "for t in filesystem_tools:\n",
        "    print(f\"  - {t.name}\")"
      ]
