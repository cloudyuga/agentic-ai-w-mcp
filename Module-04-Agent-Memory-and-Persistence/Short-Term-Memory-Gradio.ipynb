    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "from langchain.agents import AgentState\n",
        "from typing import TypedDict\n",
        "\n",
        "# Define custom state schema\n",
        "class EmployeeAgentState(AgentState):\n",
        "    \"\"\"Custom state that tracks employee information.\"\"\"\n",
        "    employee_id: str = \"\"\n",
        "    employee_name: str = \"\"\n",
        "    department: str = \"\"\n",
        "    leave_balance: int = 0\n",
        "    request_count: int = 0  # Track number of requests\n",
        "\n",
        "print(\"‚úÖ Custom state schema defined!\")\n",
        "print(\"\\nState fields:\")\n",
        "print(\"  - employee_id: Employee identifier\")\n",
        "print(\"  - employee_name: Employee full name\")\n",
        "print(\"  - department: Department name\")\n",
        "print(\"  - leave_balance: Available leave days\")\n",
        "print(\"  - request_count: Number of requests made\")"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "from langchain_core.tools import tool\n",
        "from typing import Annotated\n",
        "from langchain.agents.tool_node import InjectedState\n",
        "\n",
        "# Employee database\n",
        "EMPLOYEE_DB = {\n",
        "    \"101\": {\"name\": \"Priya Sharma\", \"department\": \"Engineering\", \"leave\": 12},\n",
        "    \"102\": {\"name\": \"Rahul Verma\", \"department\": \"Engineering\", \"leave\": 8},\n",
        "    \"103\": {\"name\": \"Anjali Patel\", \"department\": \"HR\", \"leave\": 15},\n",
        "}\n",
        "\n",
        "# Tools with state access\n",
        "@tool\n",
        "def get_my_info(state: Annotated[EmployeeAgentState, InjectedState]) -> str:\n",
        "    \"\"\"Get information about the current employee from state.\"\"\"\n",
        "    name = state.get(\"employee_name\", \"Unknown\")\n",
        "    emp_id = state.get(\"employee_id\", \"Unknown\")\n",
        "    dept = state.get(\"department\", \"Unknown\")\n",
        "    leave = state.get(\"leave_balance\", 0)\n",
        "    \n",
        "    return f\"\"\"Your Information:\n",
        "üìã Name: {name}\n",
        "üÜî ID: {emp_id}\n",
        "üè¢ Department: {dept}\n",
        "üå¥ Leave Balance: {leave} days\"\"\"\n",
        "\n",
        "@tool\n",
        "def check_leave_balance(state: Annotated[EmployeeAgentState, InjectedState]) -> str:\n",
        "    \"\"\"Check leave balance from state.\"\"\"\n",
        "    name = state.get(\"employee_name\", \"Unknown\")\n",
        "    leave = state.get(\"leave_balance\", 0)\n",
        "    return f\"{name}, you have {leave} days of leave available.\"\n",
        "\n",
        "@tool\n",
        "def request_leave(days: Annotated[int, \"Number of days\"],\n",
        "                  state: Annotated[EmployeeAgentState, InjectedState]) -> str:\n",
        "    \"\"\"Request leave days. Checks against available balance.\"\"\"\n",
        "    name = state.get(\"employee_name\", \"Unknown\")\n",
        "    current_leave = state.get(\"leave_balance\", 0)\n",
        "    \n",
        "    if days > current_leave:\n",
        "        return f\"‚ùå Sorry {name}, you only have {current_leave} days available. Cannot approve {days} days.\"\n",
        "    \n",
        "    # In production, this would update the database\n",
        "    return f\"‚úÖ Leave request submitted for {name}: {days} days. Pending manager approval.\"\n",
        "\n",
        "@tool\n",
        "def get_request_stats(state: Annotated[EmployeeAgentState, InjectedState]) -> str:\n",
        "    \"\"\"Get statistics about requests made in this session.\"\"\"\n",
        "    count = state.get(\"request_count\", 0)\n",
        "    return f\"You've made {count} request(s) in this session.\"\n",
        "\n",
        "print(\"‚úÖ Tools with state access created!\")"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "from langchain.agents import create_agent\n",
        "from langgraph.checkpoint.memory import InMemorySaver\n",
        "\n",
        "# Hooks for state management\n",
        "def post_model_hook(state: EmployeeAgentState) -> dict:\n",
        "    \"\"\"Update request count after each interaction.\"\"\"\n",
        "    current_count = state.get(\"request_count\", 0)\n",
        "    return {\"request_count\": current_count + 1}\n",
        "\n",
        "# Create agent with custom state\n",
        "employee_agent = create_agent(\n",
        "    model=\"openai:gpt-4o-mini\",\n",
        "    tools=[get_my_info, check_leave_balance, request_leave, get_request_stats],\n",
        "    state_schema=EmployeeAgentState,  # Custom state!\n",
        "    checkpointer=InMemorySaver(),\n",
        "    post_model_hook=post_model_hook,  # Track requests\n",
        "    prompt=\"\"\"You are an HR assistant that helps employees.\n",
        "    \n",
        "    The employee's information is stored in the state.\n",
        "    Use the tools to access this information.\n",
        "    \n",
        "    Be professional and helpful.\"\"\"\n",
        ")\n",
        "\n",
        "print(\"‚úÖ Employee agent with custom state created!\")"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# Test custom state\n",
        "config = {\"configurable\": {\"thread_id\": \"employee_101_session\"}}\n",
        "\n",
        "print(\"=\" * 70)\n",
        "print(\"Testing Custom State\")\n",
        "print(\"=\" * 70 + \"\\n\")\n",
        "\n",
        "# Initialize with employee data\n",
        "initial_state = {\n",
        "    \"messages\": \"Hi, I'd like to check my information\",\n",
        "    \"employee_id\": \"101\",\n",
        "    \"employee_name\": \"Priya Sharma\",\n",
        "    \"department\": \"Engineering\",\n",
        "    \"leave_balance\": 12,\n",
        "    \"request_count\": 0\n",
        "}\n",
        "\n",
        "result = employee_agent.invoke(initial_state, config)\n",
        "print(\"Turn 1: Check info\")\n",
        "print(f\"Agent: {result['messages'][-1].content}\\n\")\n",
        "\n",
        "# Request leave\n",
        "result = employee_agent.invoke(\n",
        "    {\"messages\": \"I want to request 5 days of leave\"},\n",
        "    config\n",
        ")\n",
        "print(\"Turn 2: Request leave\")\n",
        "print(f\"Agent: {result['messages'][-1].content}\\n\")\n",
        "\n",
        "# Check stats\n",
        "result = employee_agent.invoke(\n",
        "    {\"messages\": \"How many requests have I made?\"},\n",
        "    config\n",
        ")\n",
        "print(\"Turn 3: Check stats\")\n",
        "print(f\"Agent: {result['messages'][-1].content}\\n\")\n",
        "\n",
        "print(\"State values:\")\n",
        "print(f\"  Employee: {result.get('employee_name')}\")\n",
        "print(f\"  Department: {result.get('department')}\")\n",
        "print(f\"  Leave: {result.get('leave_balance')} days\")\n",
        "print(f\"  Requests: {result.get('request_count')}\")\n",
        "\n",
        "print(\"\\n‚úÖ Custom state maintained throughout conversation!\")"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "import gradio as gr\n",
        "import uuid\n",
        "\n",
        "# Session state\n",
        "current_employee = {\"id\": None, \"name\": None, \"dept\": None, \"leave\": 0}\n",
        "session_id = str(uuid.uuid4())\n",
        "\n",
        "def login_employee(employee_id):\n",
        "    \"\"\"Load employee data.\"\"\"\n",
        "    if employee_id in EMPLOYEE_DB:\n",
        "        emp = EMPLOYEE_DB[employee_id]\n",
        "        current_employee[\"id\"] = employee_id\n",
        "        current_employee[\"name\"] = emp[\"name\"]\n",
        "        current_employee[\"dept\"] = emp[\"department\"]\n",
        "        current_employee[\"leave\"] = emp[\"leave\"]\n",
        "        return f\"‚úÖ Logged in as {emp['name']} ({emp['department']})\"\n",
        "    return \"‚ùå Employee ID not found\"\n",
        "\n",
        "def chat_with_state(message, history):\n",
        "    \"\"\"Chat with custom state.\"\"\"\n",
        "    if not current_employee[\"id\"]:\n",
        "        return \"‚ö†Ô∏è Please login first using the Employee ID dropdown above.\"\n",
        "    \n",
        "    try:\n",
        "        config = {\"configurable\": {\"thread_id\": session_id}}\n",
        "        \n",
        "        # Include state in invocation\n",
        "        state = {\n",
        "            \"messages\": message,\n",
        "            \"employee_id\": current_employee[\"id\"],\n",
        "            \"employee_name\": current_employee[\"name\"],\n",
        "            \"department\": current_employee[\"dept\"],\n",
        "            \"leave_balance\": current_employee[\"leave\"],\n",
        "        }\n",
        "        \n",
        "        result = employee_agent.invoke(state, config)\n",
        "        return result['messages'][-1].content\n",
        "    except Exception as e:\n",
        "        return f\"Error: {str(e)}\"\n",
        "\n",
        "# Create Gradio interface\n",
        "with gr.Blocks(theme=gr.themes.Soft()) as demo:\n",
        "    gr.Markdown(\"# üë§ Employee Portal with Custom State - LangChain 1.0\")\n",
        "    gr.Markdown(\"\"\"This demonstrates custom agent state with employee data.\n",
        "    \n",
        "    **Features:**\n",
        "    - Employee state persists across conversation\n",
        "    - Tools access state directly (no need to pass employee ID)\n",
        "    - Request tracking in state\n",
        "    - Production-ready pattern\n",
        "    \"\"\")\n",
        "    \n",
        "    with gr.Row():\n",
        "        employee_dropdown = gr.Dropdown(\n",
        "            choices=list(EMPLOYEE_DB.keys()),\n",
        "            label=\"Employee ID\",\n",
        "            info=\"Select your employee ID to login\"\n",
        "        )\n",
        "        login_btn = gr.Button(\"Login\")\n",
        "    \n",
        "    login_status = gr.Textbox(label=\"Status\", interactive=False)\n",
        "    \n",
        "    chatbot = gr.ChatInterface(\n",
        "        fn=chat_with_state,\n",
        "        examples=[\n",
        "            \"Show me my information\",\n",
        "            \"How much leave do I have?\",\n",
        "            \"I want to request 3 days of leave\",\n",
        "            \"How many requests have I made?\",\n",
        "            \"What department am I in?\"\n",
        "        ],\n",
        "    )\n",
        "    \n",
        "    login_btn.click(login_employee, inputs=[employee_dropdown], outputs=[login_status])\n",
        "    \n",
        "    gr.Markdown(\"### Available Employees\")\n",
        "    employee_list = \"\\n\".join([f\"- **{emp_id}**: {emp['name']} ({emp['department']})\" \n",
        "                               for emp_id, emp in EMPLOYEE_DB.items()])\n",
        "    gr.Markdown(employee_list)\n",
        "\n",
        "print(f\"Session ID: {session_id}\")\n",
        "print(\"\\nAvailable employees:\")\n",
        "for emp_id, emp in EMPLOYEE_DB.items():\n",
        "    print(f\"  - {emp_id}: {emp['name']} ({emp['department']}, {emp['leave']} days leave)\")\n",
        "\n",
        "# Launch the interface\n",
        "demo.launch()"
      ]
    },
